    "test": "./test/test.sh"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "devDependencies": {
    "orchid-orm": "^1.23.11",
    "orchid-orm-schema-to-zod": "^0.6.11",
    "orchid-orm-test-factory": "^0.5.11",
    "rake-db": "^2.15.10",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  }
}
