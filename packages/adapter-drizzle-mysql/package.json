{
  "name": "@next-auth/drizzle-adapter-mysql",
  "version": "0.0.1",
  "description": "Drizzle adapter for next-auth.",
  "homepage": "https://authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "author": "Anthony Shew",
  "main": "dist/index.js",
  "license": "ISC",
  "keywords": [
    "next-auth",
    "next.js",
    "oauth",
    "drizzle"
  ],
  "private": false,
  "publishConfig": {
