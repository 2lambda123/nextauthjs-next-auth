{
  "name": "@auth/astro",
  "description": "Authentication for Astro.",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm providers",
    "clean": "rm -rf src/lib/providers",
    "providers": "node ../utils/scripts/providers.js --out src/lib"
  },
  "files": [
    "api",
    "providers",
    "src",
    "*.d.ts"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/config.ts",
    "./client": "./src/client.ts",
    "./server": "./src/server.ts",
    "./middleware": "./src/middleware.ts",
    "./providers": "./src/lib/providers/index.ts",
    "./providers/*": "./src/lib/providers/*.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "set-cookie-parser": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/set-cookie-parser": "^2.4.7",
    "astro": "^4.2.4",
    "vite": "^5.0.12"
  },
  "peerDependencies": {
    "astro": "^4.0.0"
  },
  "keywords": [
    "astro",
    "withastro",
    "astro-component",
    "authjs",
    "auth"
  ],
  "author": "Reuben Tier <otterlord.dev@gmail.com>",
  "repository": "https://github.com/nextauthjs/next-auth",
  "license": "ISC"
}
