{
  "name": "@auth/grafbase-adapter",
  "version": "0.1.0",
  "description": "Grafbase adapter for next-auth.",
  "homepage": "https://authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "author": "Jamie Barton <jamie@notrab.dev>",
  "contributors": [
    "Thang Huu Vu <hi@thvu.dev>"
  ],
  "license": "ISC",
  "keywords": [
    "next-auth",
    "@auth",
    "Auth.js",
    "next.js",
    "oauth",
    "grafbase",
    "graphql"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "files": [
    "*.d.ts*",
    "*.js",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@auth/adapter-test": "workspace:*",
    "@auth/tsconfig": "workspace:*",
    "jest": "^27.4.3",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "@auth/adapter-test/jest"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0"
  }
}
