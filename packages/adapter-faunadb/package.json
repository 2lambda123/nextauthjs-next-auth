    "dev": "tsc -w",
    "clean": "rm -rf dist",
    "test": "./tests/test.sh"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "@fauna-labs/fauna-schema-migrate": "^2.1.3",
    "fauna": "^1.0.1",
    "faunadb": "^4.8.0",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@next-auth/adapter-test": "workspace:*",
    "@next-auth/tsconfig": "workspace:*",
    "jest": "^27.4.3"
  },
  "jest": {
    "preset": "@next-auth/adapter-test/jest"
