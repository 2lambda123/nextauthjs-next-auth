{
  "name": "@auth/remix",
  "description": "Authentication for Remix.",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "client.*",
    "index.*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.js && node scripts/postbuild",
    "patch": "npm version patch --no-git-tag-version",
    "clean": "rm -rf client.* index.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@auth/core": "workspace:*",
    "@types/node": "^18.7.14",
    "next-auth": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@auth/core": "~0.2.2 || ^0.2.2",
    "@remix-run/react": "^1.0.0",
    "@remix-run/server-runtime": "^1.0.0"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "keywords": [
    "Remix",
    "React",
    "Auth"
  ],
  "author": "Corey Jepperson <corey.jepperson@gmail.com>",
  "repository": "https://github.com/nextauthjs/next-auth",
  "license": "ISC"
}