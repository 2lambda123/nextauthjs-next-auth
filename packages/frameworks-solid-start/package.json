{
  "name": "@auth/solid-start",
  "description": "Authentication for SolidStart.",
  "version": "0.7.0",
  "type": "module",
  "files": [
    "*.js",
    "*.d.ts*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./adapters": {
      "types": "./adapters.d.ts"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "patch": "npm version patch --no-git-tag-version",
    "test": "vitest run -c ../utils/vitest.config.ts",
    "clean": "rm -rf *.js *.d.ts*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "solid-js": "^1.8.15"
  },
  "keywords": [
    "SolidJS",
    "SolidStart",
    "Auth"
  ],
  "author": "OrJDev <orjdeveloper@gmail.com>",
  "repository": "https://github.com/nextauthjs/next-auth",
  "license": "ISC"
}