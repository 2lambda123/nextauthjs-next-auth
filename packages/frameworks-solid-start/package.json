{
  "name": "@auth/solid-start",
  "description": "Authentication for SolidStart.",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "client.*",
    "index.*",
    "utils.*",
    "types.*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.jsx"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "import": "./utils.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p . && rollup -c && node scripts/postbuild",
    "patch": "npm version patch --no-git-tag-version",
    "clean": "rm -rf client.* index.* types.* utils.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@auth/core": "workspace:*",
    "@solidjs/meta": "^0.28.0",
    "@types/cookie": "0.5.1",
    "@types/node": "^18.7.14",
    "@types/set-cookie-parser": "^2.4.2",
    "solid-js": "^1.5.7",
    "solid-start": "^0.2.1",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@auth/core": "~0.2.2 || ^0.2.2",
    "solid-js": "^1.5.7",
    "solid-start": "^0.2.1"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "rollup": "^3.9.1",
    "set-cookie-parser": "^2.5.1"
  },
  "keywords": [
    "SolidJS",
    "SolidStart",
    "Auth"
  ],
  "author": "OrJDev <orjdeveloper@gmail.com>",
  "repository": "https://github.com/nextauthjs/next-auth",
  "license": "ISC"
}
