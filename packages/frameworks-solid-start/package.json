{
  "name": "@auth/solid-start",
  "description": "Authentication for SolidStart.",
  "version": "0.8.0",
  "type": "module",
  "author": "Auth.js Team <authjs.dev>",
  "contributors": [
    "OrJDev <orjdeveloper@gmail.com>",
    "Nico Domino <yo@ndo.dev>"
  ],
  "license": "ISC",
  "repository": "https://github.com/nextauthjs/next-auth",
  "homepage": "https://solid.authjs.dev",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "keywords": [
    "solid",
    "solid-start",
    "auth.js"
  ],
  "scripts": {
    "dev": "pnpm providers && tsc --watch",
    "build": "pnpm clean && pnpm providers && tsc",
    "patch": "npm version patch --no-git-tag-version",
    "providers": "node ../utils/scripts/providers.js --out src/",
    "test": "vitest -c ../utils/vitest.config.ts",
    "clean": "rm -rf *.js *.d.ts* providers src/providers"
  },
  "files": [
    "*.js",
    "*.d.ts*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./adapters": {
      "types": "./adapters.d.ts"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.js"
    },
    "./providers": {
      "types": "./providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./providers/*.d.ts",
      "import": "./providers/*.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "@solidjs/router": "^0.12.3",
    "cookie": "0.6.0"
  },
  "devDependencies": {
    "@solidjs/meta": "^0.28.0",
    "@solidjs/start": "^0.5.9",
    "@types/cookie": "0.6.0",
    "@types/node": "^18.7.14",
    "solid-js": "^1.8.15"
  },
  "peerDependencies": {
    "@solidjs/start": "^0.5.9",
    "solid-js": "^1.8.15"
  }
}
