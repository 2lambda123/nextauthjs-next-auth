  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "jest": {
    "preset": "@auth/adapter-test/jest"
  },
  "devDependencies": {
    "@libsql/client": "^0.3.6"
  },
  "peerDependencies": {
    "@libsql/client": "^0.3.6"
