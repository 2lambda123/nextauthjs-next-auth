{
  "name": "@auth/pg-adapter",
  "description": "Postgres adapter for next-auth.",
  "homepage": "https://authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "author": "Jake Coppinger",
  "contributors": [
    "Thang Huu Vu <hi@thvu.dev>"
  ],
  "license": "ISC",
  "keywords": [
    "next-auth",
    "@auth",
    "Auth.js",
    "next.js",
    "oauth",
    "postgres"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "files": [
    "*.d.ts*",
    "*.js",
    "src"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "init:default": "bash create-database-tables.sh",
    "test": "pnpm init:default && jest",
    "build": "tsc"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "peerDependencies": {
    "pg": "^8"
  },
  "devDependencies": {
    "@auth/adapter-test": "workspace:*",
    "@auth/tsconfig": "workspace:*",
    "@types/pg": "^8.6.5",
    "jest": "^27.4.3",
    "pg": "^8.7.1"
  },
  "jest": {
    "preset": "@auth/adapter-test/jest"
  }
}
