{
  "name": "@auth/qwik",
  "version": "0.0.0",
  "description": "Authentication for Qwik.",
  "license": "MIT",
  "author": "gioboa <giorgiob.boa@gmail.com>",
  "homepage": "https://qwik.authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth",
  "keywords": [
    "Qwik",
    "oauth",
    "jwt",
    "oauth2",
    "authentication",
    "csrf",
    "oidc",
    "Auth.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.qwik.mjs"
    },
    "./providers": {
      "types": "./lib/providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./lib/providers/*.d.ts",
      "import": "./lib/providers/*.js"
    }
  },
  "qwik": "./lib/index.qwik.mjs",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm providers && vite build --mode lib",
    "clean": "rm -rf lib index.* src/providers",
    "test": "vitest run",
    "providers": "node ../utils/scripts/providers --out src"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.5.5",
    "@builder.io/qwik-city": "1.5.5",
    "@types/set-cookie-parser": "^2.4.7",
    "set-cookie-parser": "^2.6.0",
    "typescript": "^5.4.5",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-static-copy": "^1.0.5"
  }
}
